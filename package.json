{
  "name": "cal",
  "version": "1.0.0",
  "private": true,
  "description": "Cal let's you set reminders on a calendar view",
  "main": "index.js",
  "scripts": {
    "test": "jest -u --verbose",
    "test:watch": "jest -u --verbose --watchAll",
    "test:coverage": "jest --coverage",
    "start": "webpack-dev-server --mode development --hot --progress --color --port 3000 --open",
    "build": "webpack -p --progress --colors",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\""
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/test.adapter.ts",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "2.2.3",
    "@types/enzyme": "^3.1.10",
    "@types/history": "4.6.2",
    "@types/jest": "^22.2.3",
    "@types/node": "10.0.0",
    "@types/react": "15.0.4",
    "@types/react-dom": "16.0.5",
    "@types/react-redux": "5.0.18",
    "@types/react-router": "4.0.24",
    "@types/react-router-redux": "^5.0.13",
    "@types/redux-actions": "2.2.4",
    "@types/webpack": "4.1.4",
    "@types/webpack-env": "1.13.6",
    "awesome-typescript-loader": "^5.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^1.0.0-alpha.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss": "^6.0.21",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.4",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.2",
    "prettier": "^1.12.1",
    "react-hot-loader": "^4.1.2",
    "react-test-renderer": "^16.3.2",
    "redux-devtools-extension": "^2.13.2",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.4",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "global": "^4.3.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-drag-drawer": "^3.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.0",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.4.0"
  }
}
